                </file>    
                <file name="Export.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ExportAction"/>
                </file>    
                <file name="Zoomin.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ZoominAction"/>
                </file>
                <file name="Zoomout.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ZoomoutAction"/>
                </file>  
                <file name="ShowAll.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ShowAllAction"/>
                </file>  
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Window">
            <file name="ShowCFGEditor.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/ShowCFGEditor.instance"/>
                <attr name="position" intvalue="1300"/>
            </file>
        </folder>
    </folder> 
    
    <folder name="at-ssw-visualizer-actions-cfg">
        <file name="ShowCFGEditor.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/ShowCFGEditor.instance"/>
            <attr name="position" intvalue="300"/>
        </file>
    </folder>
       
</filesystem>
