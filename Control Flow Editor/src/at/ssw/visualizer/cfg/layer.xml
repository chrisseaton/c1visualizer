<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="OptionsDialog">
        <file name="CFGOptions.instance">
            <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.preferences.CFGOptionsCategory"/>
            <attr name="position" intvalue="1"/>
        </file>              
    </folder>    
    <folder name="Actions">
        <folder name="Window">
            <file name="ShowCFGEditor.instance">
                <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ShowCFGEditorAction"/>
            </file>
        </folder>        
        <folder name="View">      
                <file name="BezierRouter.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.UseBezierRouterAction"/>
                </file>
                <file name="DirectRouter.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.UseDirectLineRouterAction"/>
                </file>
                <file name="HierarchicalNodeLayout.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.HierarchicalNodeLayoutAction"/>
                </file>
                <file name="HierarchicalCompoundLayout.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.HierarchicalCompoundLayoutAction"/>
                </file>           
                <file name="ShowEdges.instance">
                    <attr name="instanceClass" stringvalue="at.ssw.visualizer.cfg.action.ShowEdgesAction"/>
